{
	"name": "vite-react-template",
	"description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
	"private": true,
	"cloudflare": {
		"label": "Vite React Template",
		"products": [
			"Workers"
		],
		"categories": [],
		"icon_urls": [
			"https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
		],
		"preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/fc7b4b62-442b-4769-641b-ad4422d74300/public",
		"publish": true
	},
	"dependencies": {
		"drizzle-orm": "^0.44.4",
		"hono": "4.9.4",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"react-router-dom": "^7.9.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@cloudflare/vite-plugin": "1.11.7",
		"@cloudflare/vitest-pool-workers": "^0.8.66",
		"@faker-js/faker": "^9.9.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.3.0",
		"@types/react": "19.1.11",
		"@types/react-dom": "19.1.7",
		"@vitejs/plugin-react": "5.0.1",
		"drizzle-kit": "^0.31.4",
		"jsdom": "^27.0.0",
		"react-window": "^2.1.0",
		"tsx": "^4.20.4",
		"typescript": "5.9.2",
		"typescript-eslint": "8.40.0",
		"vite": "^7.1.3",
		"vitest": "^3.2.4",
		"wrangler": "^4.32.0"
	},
	"scripts": {
		"build": "tsc -b && vite build",
		"cf-typegen": "wrangler types --config wrangler.json",
		"check": "tsc && vite build && wrangler deploy --dry-run --config wrangler.json",
		"deploy": "wrangler deploy --config wrangler.json",
		"dev": "vite",
		"dev:worker": "wrangler dev --config wrangler.json",
		"dev:vite": "vite",
		"lint": "biome check",
		"fix": "biome check --write",
		"typecheck": "tsc --noEmit",
		"preview": "npm run build && vite preview",
		"postinstall": "node scripts/postinstall-seed.mjs",
		"seed": "node scripts/postinstall-seed.mjs",
		"seed:500": "SEED_ROWS=500 node scripts/postinstall-seed.mjs",
		"migrate:local": "wrangler d1 migrations apply test-db --local --config wrangler.json",
		"migrate:prod": "wrangler d1 migrations apply test-db --config wrangler.json",
		"db:count": "wrangler d1 execute test-db --local --config wrangler.json --command \"SELECT COUNT(*) AS cnt FROM expenses;\"",
		"test": "vitest",
		"test:web": "vitest -p web",
		"test:worker": "vitest -p worker"
	},
	"type": "module"
}